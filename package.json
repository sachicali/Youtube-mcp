{
  "name": "youtube-scraping-mcp-server",
  "version": "1.0.0",
  "description": "YouTube Scraping MCP Server for analytics, transcripts, and trend analysis",
  "main": "src/index.ts",
  "scripts": {
    "dev": "wrangler dev --env development",
    "dev:staging": "wrangler dev --env staging",
    "dev:production": "wrangler dev --env production",
    "deploy": "wrangler deploy --env production",
    "deploy:staging": "wrangler deploy --env staging",
    "deploy:development": "wrangler deploy --env development",
    "build": "tsc",
    "test": "jest",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "mcp",
    "youtube",
    "analytics",
    "transcripts",
    "cloudflare-workers",
    "typescript"
  ],
  "author": "YouTube MCP Team",
  "license": "MIT",
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20241022.0",
    "@types/jest": "^29.5.0",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.5.0",
    "prettier": "^3.0.0",
    "ts-jest": "^29.1.0",
    "typescript": "^5.0.0",
    "wrangler": "^4.19.1"
  },
  "dependencies": {
    "@types/json-schema": "^7.0.15",
    "googleapis": "^126.0.0",
    "zod": "^3.22.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
